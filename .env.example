# Gemini API Key - Get yours at https://aistudio.google.com/apikey
NEXT_PUBLIC_GEMINI_API_KEY=


# Gemini Model Configuration
# Available models:
# gemini-3-flash-preview - Latest Gemini 3 (recommended)
# gemini-flash-lite - Lightweight and fast
NEXT_PUBLIC_GEMINI_MODEL=gemini-3-flash-preview

# Enable thinking mode (extended reasoning)
NEXT_PUBLIC_GEMINI_THINKING_ENABLED=false

# Thinking Budget (0-24576 tokens for 2.5-flash, 0-8192 for others)
# 0 = No thinking (lowest cost/latency)
# 8192 = Default balanced thinking
# 24576 = Maximum thinking (2.5-flash only)
NEXT_PUBLIC_GEMINI_THINKING_BUDGET=8192
